{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc && rollup -c",
    "dev": "rollup -c -w",
    "dev:help": "echo - \\\"npm run dev\\\" will run rollup in \\\"watch\\\" mode which based on rollup.config will in turn run \\\"npm start\\\" in \\\"--dev\\\" mode;",
    "start": "sirv public --single --port 5000",
    "start:help": "echo run \\\"npm run dev\\\" instead of running \\\"npm start\\\" directly.",
    "validate": "svelte-check"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@rollup/plugin-wasm": "^5.1.0",
    "@tsconfig/svelte": "^1.0.0",
    "argon2-browser": "^1.15.2",
    "express-http-proxy": "^1.6.2",
    "rollup": "^2.3.4",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-external-globals": "^0.6.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "date-fns": "^2.16.1",
    "run-sdk": "^0.6.10",
    "sirv-cli": "^1.0.0",
    "whatwg-fetch": "^3.5.0"
  }
}
